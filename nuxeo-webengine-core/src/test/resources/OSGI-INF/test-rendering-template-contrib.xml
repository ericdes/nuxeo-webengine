<?xml version="1.0"?>

<component name="org.nuxeo.ecm.webengine.rendering-template.test">

  <extension target="org.nuxeo.ecm.webengine.WebEngineComponent"
    point="application">
    <webapp id="nuxeo-web">
      <roots>
        <root priority="0">default</root>
      </roots>
      <templates>
        <template>ext1</template>
        <template>ext2</template>
      </templates>
    </webapp>
  </extension>


  <extension target="org.nuxeo.ecm.webengine.WebEngineComponent"
    point="rendering-template">

	<template name="ext1" class="org.nuxeo.ecm.webengine.tests.rendering.MyRenderingExtension"/>
	<template name="ext2" class="org.nuxeo.ecm.webengine.tests.rendering.MyRenderingExtensionFactory"/>

  </extension>

</component>
