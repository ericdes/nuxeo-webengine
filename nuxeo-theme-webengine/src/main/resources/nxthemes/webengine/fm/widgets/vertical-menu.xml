<div>
<ul>
  <#list nxthemesInfo.model.items as item>
    <li>
      <#if item.icon>
        <img src="${item.icon}" />
      </#if>
      <#if item.selected>
        <a class="selected" href="${item.url}">${item.title}</a>
      <#else>
        <a href="${item.url}">${item.title}</a>    
      </#if>
    </li>
  </#list>
</ul>
</div>
