<#macro pageLogo>

<div class="logo" >
  <div style="vertical-align: middle;">
    <table >
      <tr style="vertical-align: top;">
        <td style="padding-left: 10px;text-align: left;">
          <div class="pageTitle">
            ${pageName}
          </div>
          <div class="pageDescription">
            ${description} 
          </div>
        </td>
      </tr>
    </table>
  </div>
</div>

</#macro>